<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include
        android:id="@+id/lay_info"
        layout="@layout/l_user_info" />

    <TextView
        android:id="@+id/my_resume"
        style="@style/User_Info_TextView"
        android:text="@string/resume" />

    <TextView
        android:id="@+id/my_deliver"
        style="@style/User_Info_TextView"
        android:text="@string/my_deliver" />

    <TextView
        android:id="@+id/deliver_set"
        style="@style/User_Info_TextView"
        android:text="@string/set_deliver" />

    <TextView
        android:id="@+id/umeng_fb"
        style="@style/User_Info_TextView"
        android:text="@string/umeng_fb" />

    <TextView
        android:id="@+id/change_user"
        style="@style/User_Info_TextView"
        android:text="@string/change_user" />

</LinearLayout>